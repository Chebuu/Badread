window_identity_plot <- function(data, title, x_lab, y_lab) {
data$window_identity <- data$window_identity / 100.0
p <- ggplot(data = data, aes(x=pos, y=window_identity, colour=read)) +
geom_line(alpha=0.75) +
# scale_colour_manual(values = rev(brewer.pal(10,"Spectral"))) +
scale_colour_manual(values = c("#5E4FA2", "#3288BD", "#66C2A5", "#A9D6A1", "#DBE591", "#EFD084", "#FDAE61", "#F46D43", "#D53E4F", "#9E0142")) +
theme_bw() +
theme(legend.position="none") +
scale_x_continuous(expand=c(0, 0)) +
scale_y_continuous(expand=c(0, 0), labels = percent) +
coord_cartesian(xlim=c(0, 10000), ylim=c(0.6, 1)) +
labs(title = title, x = x_lab, y = y_lab)
return(p)
}
